{{"{% extends \"::base.html.twig\" %}"}}
{{"{% set subtitle = '"~entity~" Edit (New)' %}"}}

{{"{% block main %}"}}
{{"
   {% for flashMessage in app.session.flashbag.get('notice') %}
        {% if flashMessage is defined %}
            <div class=\"flash-notice csuccess\">
                <h4>{{ flashMessage }}</h4>
            </div>
        {% endif %}
    {% endfor %}
"}}
{{ "<div  class="c990000 bold error">" }}
{{ "{{ form_errors(form) }}" }}
{{ "</div>" }}
<form action="{{ "{{ path('"~ route_name_prefix ~"_create') }}" }}" method="post" {{ "{{ form_enctype(form) }}" }}>
    <div class="span12">
        <div  class="clear">
        </div>
{% for field in fieldnames %}
{% if field != "id" %}
        <div class="row-fluid">
            <div  class="span2">
                {{ "{{form_label(form."}}{{field}}{{") }}"}}
            </div>
            <div  class="span6">
                {{ "{{form_widget(form."}}{{field}}{{") }}"}}
            </div>
        </div>
{% endif %}
{% endfor %}
{% for field in checks %}
        <div class="row-fluid">
            <div  class="span2">
                {{ "{{form_label(form."}}{{field}}{{") }}"}}
            </div>
            <div  class="span6">
                {{ "{{form_widget(form."}}{{field}}{{") }}"}}
            </div>
        </div>
{% endfor %}
{% for field in maps %}
        <div class="row-fluid">
            <div  class="span2">
                {{ "{{form_label(form."}}{{field}}{{") }}"}}
            </div>
            <div  class="span6">
                {{ "{{form_widget(form."}}{{field}}{{") }}"}}
            </div>
        </div>
{% endfor %}
    </div>
    {{ "<div  class="span12 clear error hidden">" }}
    {{ "{{ form_rest(form) }}" }}
    {{ "</div>" }}
    <p>
        <button type="submit" class="btn btn-primary bold">Save</button>
    </p>
</form>
{{"{% endblock main %"}}
